{
	"name": "chillerlan/gw2-database",
	"description": "A Guild Wars 2 Database built from the official API. PHP 7",
	"homepage": "https://github.com/codemasher/gw2-database",
	"license": "MIT",
	"type": "library",
	"keywords": [
		"guild wars 2", "API", "mysql", "database"
	],
	"authors": [
		{
			"name": "Smiley",
			"email": "smiley@chillerlan.net"
		}
	],
	"require": {
		"php": ">=7.0.3",
		"chillerlan/database": "dev-master",
		"chillerlan/php-curl": "dev-master",
		"vlucas/phpdotenv": "2.*"
	},
	"require-dev": {
		"phpunit/phpunit": "5.5.*"
	},
	"suggest": {
		"chillerlan/bbcode": "for use with GW2BBCode"
	},
	"autoload": {
		"files": [
			"src/Helpers/functions.php"
		],
		"psr-4": {
			"chillerlan\\GW2DBCLI\\": "cli/",
			"chillerlan\\GW2DB\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"chillerlan\\GW2DBExamples\\": "examples/"
		}
	}
}
